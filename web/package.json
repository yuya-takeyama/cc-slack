{
  "name": "cc-slack-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --unsafe src/ *.js *.json styles/",
    "lint:fix": "biome check --write --unsafe src/ *.js *.json styles/",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "all": "pnpm typecheck && pnpm lint && pnpm format && pnpm test"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.7.1",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.6",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.6"
  }
}
